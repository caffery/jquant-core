<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:util="http://www.springframework.org/schema/util"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd" >

<bean id="cacheAdministrator" class="com.opensymphony.oscache.general.GeneralCacheAdministrator" destroy-method="destroy">
    <constructor-arg index="0">
        <props>
            <prop key="cache.memory">true</prop>
        </props>
    </constructor-arg>
</bean>


<bean id="simpleExtractor" class="org.springframework.jdbc.support.nativejdbc.SimpleNativeJdbcExtractor"/>


<bean id="dataSource" class="org.jquant.data.datasource.OracleDataSourceFactoryBean">
	<property name="minLimit" value="5"/>
	<property name="maxLimit" value="30"/>
	<property name="initialLimit" value="5"/>
</bean>
	
<bean id="sqlMapClient" class="org.springframework.orm.ibatis.SqlMapClientFactoryBean">
	<property name="configLocation" value="sqlmap-config.xml"/>
	<property name="dataSource" ref="dataSource"/>
</bean>


<bean id="tickerQuoteDAO" class="org.jquant.provider.louxor.dao.TickerQuoteDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="tickerDAO" class="org.jquant.provider.louxor.dao.TickerDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="volatilityDAO" class="org.jquant.provider.louxor.dao.VolatilityDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="tickerCdsDAO" class="org.jquant.provider.louxor.dao.TickerCdsDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="tickerForexDAO" class="org.jquant.provider.louxor.dao.TickerForexDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="tickerTimeDepositDAO" class="org.jquant.provider.louxor.dao.TickerTimeDepositDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="tickerVarSwapDAO" class="org.jquant.provider.louxor.dao.TickerVarSwapDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="tickerIndexDAO" class="org.jquant.provider.louxor.dao.TickerIndexDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="tickerStockDAO" class="org.jquant.provider.louxor.dao.TickerStockDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="tickerRateMaturityDAO" class="org.jquant.provider.louxor.dao.TickerRateMaturityDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="lxrCashDividendDAO" class="org.jquant.provider.louxor.dao.LxrCashDividendDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>

<bean id="property" class="org.springframework.beans.factory.config.PropertiesFactoryBean">
	<property name="location"><value>louxorSymbols.properties</value></property>
</bean>

<bean id="timeSerieReader" class="org.jquant.provider.louxor.LouxorTimeSerieReader">
	<property name="tickerQuoteDao" ref="tickerQuoteDAO"/>
	<property name="volatilityDao" ref="volatilityDAO"/>
	<property name="lxrCashDividendDao" ref="lxrCashDividendDAO"/>
</bean>

<bean id="instrumentFactory" class="org.jquant.factory.InstrumentFactory">
	<property name="reader" ref="timeSerieReader"/>

</bean>

<bean id="curveFactory" class="org.jquant.factory.TermStructureFactory">
	<property name="reader" ref="timeSerieReader"/>

</bean>

<bean id="volatilityStructureFactory" class="org.jquant.factory.VolatilityStructureFactory">
	<property name="reader" ref="timeSerieReader"/>
</bean>

<bean id="calibrationModelFactory" class="org.jquant.factory.CalibrationModelFactory">
	<property name="reader" ref="timeSerieReader"/>
</bean>

<bean id="hestonParametersDAO" class="org.jquant.provider.louxor.dao.HestonParametersDAOImpl">
	<property name="sqlMapClient" ref="sqlMapClient"/>
</bean>


</beans>